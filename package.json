{
  "name": "cypressoctoberv12",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "triggerAllTests-headless": "npx cypress run",
    "triggerAllTests-headed": "npx cypress run --headed",
    "triggerAllTests-chrome": "npx cypress run --browser chrome --headed",
    "triggerAllTests-dashboard": "npx cypress run --record --key 6aa39e6e-aade-44e2-9c77-59decf447397",
    "triggerAllTests-webdriveruni": "npx cypress run --spec cypress/e2e/webdriver-uni",
    "junit-merge": "npx junit-merge -d cypress/results -o cypress/results/results.xml ",
    "delete-junit-report": "rm -rf cypress/results/results.xml",
    "delete-results": "rm -rf cypress/results/*.xml",
    "mochawesome-merge": "npx mochawesome-merge cypress/results/*.json > mochawesome.json && npx marge mochawesome.json",
    "delete-mochawesome-report": "rm -rf mochawesome-report/* || true",
    "cypress-multi-browser": "npm run triggerAllTests-webdriveruni -- --browser electron --headed & npm run triggerAllTests-webdriveruni -- --browser chrome --headed "

  },
  "author": "Romed",
  "license": "ISC",
  "dependencies": {
    "cypress": "^13.15.0",
    "cypress-drag-drop": "^1.1.1"
  },
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "@faker-js/faker": "^9.1.0",
    "cypress-multi-reporters": "^2.0.4",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "mocha": "10.8.2"

  }
}
